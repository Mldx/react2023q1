    "@types/node": "^18.11.12",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/redux-mock-store": "^1.0.3",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "eslint": "^8.36.0",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.59.3",
    "typescript": "^4.9.3",
    "unsplash-js": "^7.0.15",
